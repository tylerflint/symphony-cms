php_extensions:
    - mysql
    - xsl
    
url_rewrites:
    - 
        rule: "^/symphony/(.*)$ /symphony/index.php?page=$1 last"
        conditions: 
            - "$request_filename ~ /symphony/"
    - 
        rule: "^/image/(.+\.(jpg|gif|jpeg|png|bmp|JPG|GIF|JPEG|PNG|BMP))$ /extensions/jit_image_manipulation/lib/image.php?param=$1 last"
        conditions:
            - "!-f $request_filename"
    - 
        rule: "^/(.*[^/]+)$ /$1/ permanent"
        conditions:
            - "!-f $request_filename"
    -
        rule: "^/(.*)$ /index.php?page=$1 last"
        conditions:
            - "!-d $request_filename"